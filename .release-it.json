{
    "requireUpstream": false,
    "requireCleanWorkingDir": false,
    "github": {
      "release": false
    },
    "git": {
      "commit": true,
      "push": true,
      "pushArgs": ["--follow-tags", "-f"],
      "requireUpstream": false,
      "requireCleanWorkingDir": false,
      "tagName": "v${version}",
      "commitMessage": "[automate_versioning] Release ${version}"
    },
    "npm": {
      "publish": false
    }
  }